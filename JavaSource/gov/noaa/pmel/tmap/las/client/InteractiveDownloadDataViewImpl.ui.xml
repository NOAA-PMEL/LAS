<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui'>
	<ui:style>
		
	</ui:style>

	<g:Grid width="472px" height="772px">
		<g:row>
			<g:customCell>
				<g:HTML wordWrap="true" width="37px"/>
			</g:customCell>
			<g:customCell>
				<g:FlowPanel width="400px" height="700px" ui:field="iddPanel">
					<g:HTML wordWrap="true" ui:field="lblDownloadData"
						horizontalAlignment="ALIGN_CENTER">
						<h2>Specify your data's requirements and then click "Save" to
							download.</h2>
					</g:HTML>
					<g:HTML wordWrap="true" ui:field="lblSelectedRegion">
						<strong>Selected Region:</strong>
					</g:HTML>
					<g:Label text="Longitude Range: [,]" ui:field="selectedRegionLongitude" />
					<g:Label text="Latitude Range: [,]" ui:field="selectedRegionLatitude" />
					<g:HTML wordWrap="true" ui:field="lblSelectAData">
						<strong>Select a Data Format:</strong>
					</g:HTML>
					<g:ListBox visibleItemCount="1" ui:field="dataFormatComboBox"
						name="dataFormatComboBox" enabled="false" selectedIndex="0" />
					<g:FlowPanel ui:field="dateTimePanel" visible="false">
						<g:HTML wordWrap="true" ui:field="lblSelectTime">
							<strong>Select Time:</strong>
						</g:HTML>
					</g:FlowPanel>
					<g:FlowPanel ui:field="axisPanel" visible="false">
						<g:HTML wordWrap="true" ui:field="lblSelectDepth">
							<strong>Select Depth:</strong>
						</g:HTML>
					</g:FlowPanel>
					<g:Button ui:field="saveButton" enabled="false" text="Save" />
				</g:FlowPanel>
			</g:customCell>
			<g:customCell>
				<g:HTML wordWrap="true" width="37px"/>
			</g:customCell>
		</g:row>
	</g:Grid>
</ui:UiBinder>
