                <variable>
                        <name>FER_FONTS</name>
                        <value>/home/porter/tmap/ferret/linux/ppl/fonts</value>
                </variable>
                <!-- before ferret 7.2 -->
                <variable>
                        <name>PLOTFONTS</name>
